let makeodd = proc (makeeven, makeodd)
                 proc (x)
                  if zero?(x)
                  then zero?(1)
                  else ((makeeven makeeven makeodd) -(x,1))
    makeeven = proc (makeeven, makeodd)
                 proc (x)
                  if zero?(x)
                  then zero?(0)
                  else ((makeodd makeeven makeodd) -(x,1))
in let odd = proc (x) ((makeodd makeeven makeodd) x)
   in (odd 3)
